<label><%= notice %></label>
<% if @contact.errors.any? %>
    <div id="error_explanation">
      <h2><%= @contact.errors.count %>errors occurred.</h2>
      <ul>
      <% @contact.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
<% end %>


<%= form_with(model: @contact, local: true) do |form| %>



  <div class="">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="">
    <%= form.label :email %>
    <%= form.text_field :email %>
  </div>
  <div class="">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <%= form.submit %>
<% end %>
 <br><br><br>
 <%= link_to 'Back to list', contacts_path %>